{
    "name": "Paragraph Background Color BG Scripts",
    "manifest_version": 2,
    "version": "0.001",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "logo512.png"
    }
}
